  object zusqlCaixas: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM caixas'
      'WHERE'
      '  ((caixas.ID IS NULL AND :OLD_ID IS NULL) OR (caixas.ID = :OLD_ID))'
    )
