      'INSERT INTO caixas'
      '  (ID, DATA_ABERTURA, DATA_FECHAMENTO, VALOR_ABERTURA, VALOR_FECHAMENTO, '
      '   ATIVO, USUARIO)'
      'VALUES'
      '  (:ID, :DATA_ABERTURA, :DATA_FECHAMENTO, :VALOR_ABERTURA, :VALOR_FECHAMENTO, '
